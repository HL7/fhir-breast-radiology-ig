  "description": "Mammography Breast Composition",
  "compose": {
    "include": [
      {
        "system": "http://hl7.org/fhir/us/breast-radiology/CodeSystem/BreastDensityCS",
        "concept": [
          {
            "code": "1",
            "display": "a. The breasts are almost entirely fatty"
          },
          {
            "code": "2",
            "display": "b. There are scattered areas of fibroglandular density"
          },
          {
            "code": "3",
            "display": "c. The breasts are heterogeneously dense, which may obscure detection of small masses"
          },
          {
            "code": "4",
            "display": "d. The breasts are extremely dense, which lowers the sensitivity of mammography"
          }
        ]
      }
    ]
  }
